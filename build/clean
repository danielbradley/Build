#!/bin/bash
#
#	This script removes all the object files, libraries and
#	shared objects created by "compile"
#

RM=rm

VERSION=0.5
SUFFIX=5

$RM -rf bin
$RM -rf obj
$RM -rf providers/{lib,obj}
$RM -rf dep/libislabs_build/{lib,obj}
$RM -rf dep/OpenOCL_PSA/libopenocl_base_psa/{lib,obj}
$RM -rf dep/OpenOCL_PSA/libopenocl_io_psa/{lib,obj}
$RM -rf dep/OpenOCL_PSA/libopenocl_provider_psa/{lib,obj}
$RM -rf dep/OpenOCL_PSA/libopenocl_threads_psa/{lib,obj}
